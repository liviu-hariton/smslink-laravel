    "keywords": ["sms", "smslink", "smslink-laravel", "smslink-api", "laravel"],
    "authors": [
        {
            "name": "Liviu Hariton",
            "email": "liviu.hariton@gmail.com",
            "homepage": "https://eu.liviuhariton.com",
            "role": "Developer"
        }
    ],
    "support": {
        "source": "https://github.com/liviu-hariton/smslink-laravel",
        "issues": "https://github.com/liviu-hariton/smslink-laravel/issues"
    },
    "type": "library",
    "require": {
        "php": ">=8.3",
        "laravel/framework": ">=11.0"
    },
    "license": "MIT",
    "autoload": {
